<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:angle="270.0" android:endColor="@color/app_theme_transparent_two" android:startColor="@color/app_theme_transparent_two" />
            <stroke android:width="1.0px" android:color="@color/app_line_divider_gray" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:angle="270.0" android:endColor="@color/app_theme_transparent_two" android:startColor="@color/app_theme_transparent_two" />
            <stroke android:width="1.0px" android:color="@color/app_line_divider_gray" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:angle="270.0" android:endColor="@color/app_white" android:startColor="@color/app_white" />
            <stroke android:width="1.0px" android:color="@color/app_line_divider_gray" />
        </shape>
    </item>
</selector>